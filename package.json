{
  "name": "galaxiaone721",
  "version": "1.0.0",
  "description": "Non-fungible token standard for the universe",
  "scripts": {
    "compile": "specron compile",
    "sandbox": "specron sandbox",
    "prepare": "specron compile",
    "test": "specron compile && specron test",
    "solhint": "solhint src/**/*.sol"
  },
  "specron": {
    "compiler": {
      "build": "./build",
      "match": [
        "./src/**/*.sol"
      ]
    },
    "test": {
      "match": [
        "./src/**/*.test.*"
      ]
    },
    "require": [
      "ts-node/register"
    ]
  },
  "author": "Kyle Dewhurst",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/galaxiaone/721.git"
  },
  "bugs": {
    "url": "https://github.com/galaxiaone/721/issues"
  },
  "homepage": "https://github.com/galaxiaone/721#readme",
  "keywords": [
    "ethereum",
    "blockchain",
    "eth",
    "contract",
    "contracts",
    "smart",
    "smart-contract",
    "smart-contracts",
    "token",
    "tokens",
    "nft",
    "nfts",
    "non-fungible",
    "non-fungibles",
    "eip",
    "erc",
    "erc721",
    "erc-721",
    "deed",
    "standard"
  ],
  "dependencies": {
    "@specron/cli": "0.5.6",
    "@specron/spec": "0.5.6",
    "@truffle/hdwallet-provider": "^1.0.18",
    "cryptiles": "4.1.2",
    "dotenv": "~8.1",
    "ethers": "~4.0",
    "node-wget": "^0.4.3",
    "sol-verifier": "^2.1.0",
    "solc": "0.5.6",
    "solhint": "~1.4",
    "truffle-flattener": "^1.4.2",
    "truffle-hdwallet-provider": "1.0.17",
    "ts-node": "^8.3.0",
    "typescript": "~3.6.3",
    "web3": "1.2.1"
  }
}
